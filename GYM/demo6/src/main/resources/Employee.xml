<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.model.Employee">
        <table name="employee" schema="gym" catalog=""/>
        <attributes>
            <id name="idemp">
                <column name="idemp"/>
            </id>
            <basic name="tenemp">
                <column name="tenemp"/>
            </basic>
            <basic name="sdtemp">
                <column name="sdtemp"/>
            </basic>
            <basic name="emailemp">
                <column name="emailemp"/>
            </basic>
            <basic name="genderemp">
                <column name="genderemp"/>
            </basic>
            <basic name="positionemp">
                <column name="positionemp"/>
            </basic>
            <basic name="passemp">
                <column name="passemp"/>
            </basic>
            <basic name="createemp">
                <column name="createemp" nullable="false" precision="-1"/>
            </basic>
            <basic name="avatar">
                <column name="avatar" nullable="false" length="1000"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
