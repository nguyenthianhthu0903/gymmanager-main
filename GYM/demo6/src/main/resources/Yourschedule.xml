<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.model.Yourschedule">
        <table name="yourschedule" schema="gym" catalog=""/>
        <attributes>
            <id name="stt">
                <column name="stt" precision="11"/>
            </id>
            <basic name="idkhachhang">
                <column name="idkhachhang"/>
            </basic>
            <basic name="title">
                <column name="title"/>
            </basic>
            <basic name="start">
                <column name="start"/>
            </basic>
            <basic name="end">
                <column name="end"/>
            </basic>
            <basic name="confirmed">
                <column name="confirmed" nullable="false"/>
            </basic>
            <basic name="reason">
                <column name="reason"/>
            </basic>
            <basic name="location">
                <column name="location"/>
            </basic>
            <basic name="resource">
                <column name="resource" nullable="false" precision="11"/>
            </basic>
            <basic name="timecheck">
                <column name="timecheck" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Teacher">
        <table name="teacher" schema="gym" catalog=""/>
        <attributes>
            <id name="idteacher">
                <column name="idteacher"/>
            </id>
            <basic name="nameteacher">
                <column name="nameteacher"/>
            </basic>
            <basic name="emailteacher">
                <column name="emailteacher"/>
            </basic>
            <basic name="sdtteacher">
                <column name="sdtteacher"/>
            </basic>
            <basic name="genderteacher">
                <column name="genderteacher"/>
            </basic>
            <basic name="passteacher">
                <column name="passteacher"/>
            </basic>
            <basic name="createteacher">
                <column name="createteacher" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Teacher">
        <table name="teacher" schema="gym" catalog=""/>
        <attributes>
            <id name="idteacher">
                <column name="idteacher"/>
            </id>
            <basic name="nameteacher">
                <column name="nameteacher"/>
            </basic>
            <basic name="emailteacher">
                <column name="emailteacher"/>
            </basic>
            <basic name="sdtteacher">
                <column name="sdtteacher"/>
            </basic>
            <basic name="genderteacher">
                <column name="genderteacher"/>
            </basic>
            <basic name="passteacher">
                <column name="passteacher"/>
            </basic>
            <basic name="createteacher">
                <column name="createteacher" nullable="false" precision="-1"/>
            </basic>
            <basic name="avatar">
                <column name="avatar" nullable="false" length="1000"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.example.demo.model.Khachhang">
        <table name="khachhang" schema="gym" catalog=""/>
        <attributes>
            <id name="makh">
                <column name="makh"/>
            </id>
            <basic name="tenkh">
                <column name="tenkh"/>
            </basic>
            <basic name="sdtkh">
                <column name="sdtkh"/>
            </basic>
            <basic name="emailkh">
                <column name="emailkh"/>
            </basic>
            <basic name="gender">
                <column name="gender"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="role">
                <column name="role" nullable="false"/>
            </basic>
            <basic name="level">
                <column name="level" nullable="false"/>
            </basic>
            <basic name="point">
                <column name="point" nullable="false" precision="255"/>
            </basic>
            <basic name="createaccount">
                <column name="createaccount" nullable="false" precision="-1"/>
            </basic>
            <basic name="status">
                <column name="status"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
