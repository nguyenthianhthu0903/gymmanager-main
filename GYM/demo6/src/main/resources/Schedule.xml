<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.demo.model.Schedule">
        <table name="schedule" schema="gym" catalog=""/>
        <attributes>
            <id name="stt">
                <column name="stt" precision="255"/>
            </id>
            <basic name="city">
                <column name="city"/>
            </basic>
            <basic name="idclb">
                <column name="idclb"/>
            </basic>
            <basic name="ngay">
                <column name="ngay"/>
            </basic>
            <basic name="bomon">
                <column name="bomon"/>
            </basic>
            <basic name="img">
                <column name="img"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
