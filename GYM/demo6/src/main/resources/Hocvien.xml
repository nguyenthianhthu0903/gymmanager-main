            </id>
            <basic name="mahocvien">
                <column name="mahocvien"/>
            </basic>
            <basic name="tenhocvien">
                <column name="tenhocvien"/>
            </basic>
            <basic name="bomondk">
                <column name="bomondk"/>
            </basic>
            <basic name="thoigiandk">
                <column name="thoigiandk"/>
            </basic>
            <basic name="thoigianketthuc">
                <column name="thoigianketthuc"/>
            </basic>
            <basic name="createdkhocvien">
                <column name="createdkhocvien" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
